@article{RN114,
   author = {Mao, L. and Guo, P. and Kepenekian, M. and Spanopoulos, I. and He, Y. and Katan, C. and Even, J. and Schaller, R. D. and Seshadri, R. and Stoumpos, C. C. and Kanatzidis, M. G.},
