@article{RN617,
   author = {Bhatt, M. and Nayak, P. K. and Ghosh, D.},
   title = {Data-Driven Design of Electroactive Spacer Molecules to Tune Charge Carrier Dynamics in Layered Halide Perovskite Heterostructures},
   journal = {ACS Nano},
   volume = {18},
