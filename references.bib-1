   title = {Predictions of moire excitons in twisted two-dimensional organic-inorganic halide perovskites},
   journal = {Chem Sci},
   volume = {12},
   number = {17},
   pages = {6073-6080},
   note = {Zhang, Linghai
Zhang, Xu
Lu, Gang
eng
England
2021/05/18
Chem Sci. 2021 Mar 23;12(17):6073-6080. doi: 10.1039/d1sc00359c.},
   abstract = {Recent breakthrough in synthesizing arbitrary vertical heterostructures of Ruddlesden-Popper (RP) perovskites opens doors to myriad quantum optoelectronic applications. However, it is not clear whether moire excitons and flat bands can be formed in such heterostructures. Here, we predict from first principles that twisted homobilayers of RP perovskite, MA2PbI4, can host moire excitons and yield flat energy bands. The moire excitons exhibit unique and hybridized characteristics with electrons confined in a single layer of a striped distribution while holes localized in both layers. Nearly flat valence bands can be formed in the bilayers with relatively large twist angles, thanks to the presence of hydrogen bonds that strengthen the interlayer coupling. External pressures can further increase the interlayer coupling, yielding more localized moire excitons and flatter valence bands. Finally, electrostatic gating is predicted to tune the degree of hybridization, energy, position and localization of moire excitons in twisted MA2PbI4 bilayers.},
   ISSN = {2041-6520 (Print)
2041-6520 (Linking)},
   DOI = {10.1039/d1sc00359c},
   url = {https://www.ncbi.nlm.nih.gov/pubmed/33996003},
   year = {2021},
   type = {Journal Article}
}

