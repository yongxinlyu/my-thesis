   year = {2019},
   type = {Journal Article}
}

@article{RN625,
   author = {Yin, J. and Yang, H. and Gutierrez-Arzaluz, L. and Zhou, Y. and Bredas, J. L. and Bakr, O. M. and Mohammed, O. F.},
