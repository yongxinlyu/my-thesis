   author = {Peng, Y. and Liu, X. and Li, L. and Yao, Y. and Ye, H. and Shang, X. and Chen, X. and Luo, J.},
   title = {Realization of vis-NIR Dual-Modal Circularly Polarized Light Detection in Chiral Perovskite Bulk Crystals},
   journal = {J Am Chem Soc},
