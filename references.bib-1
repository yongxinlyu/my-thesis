photovoltaics
solar cells},
   ISSN = {1521-3773 (Electronic)
1433-7851 (Linking)},
   DOI = {10.1002/anie.201406466},
   url = {https://www.ncbi.nlm.nih.gov/pubmed/25196933},
   year = {2014},
   type = {Journal Article}
}

