}

@article{RN559,
   author = {Hedman, D. and McLean, B. and Bichara, C. and Maruyama, S. and Larsson, J. A. and Ding, F.},
   title = {Dynamics of growing carbon nanotube interfaces probed by machine learning-enabled molecular simulations},
   journal = {Nat Commun},
   volume = {15},
