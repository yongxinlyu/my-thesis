   year = {2018},
   type = {Journal Article}
}

@article{RN121,
   author = {Li, X. and Fu, Y. and Pedesseau, L. and Guo, P. and Cuthriell, S. and Hadar, I. and Even, J. and Katan, C. and Stoumpos, C. C. and Schaller, R. D. and Harel, E. and Kanatzidis, M. G.},
   title = {Negative Pressure Engineering with Large Cage Cations in 2D Halide Perovskites Causes Lattice Softening},
   journal = {J Am Chem Soc},
   volume = {142},
   number = {26},
   pages = {11486-11496},
   note = {Li, Xiaotong
Fu, Yongping
