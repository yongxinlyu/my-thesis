   year = {2022},
   type = {Journal Article}
}

@article{RN404,
   author = {Okamoto, T. and Yu, C. P. and Mitsui, C. and Yamagishi, M. and Ishii, H. and Takeya, J.},
   title = {Bent-Shaped p-Type Small-Molecule Organic Semiconductors: A Molecular Design Strategy for Next-Generation Practical Applications},
