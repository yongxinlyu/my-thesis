@article{RN620,
   author = {Mahal, E. and Mandal, S. C. and Roy, D. and Pathak, B.},
   title = {Energy level alignments between organic and inorganic layers in 2D layered perovskites: conjugation vs. substituent},
   journal = {Nanoscale},
   volume = {15},
