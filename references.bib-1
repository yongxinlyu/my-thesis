@article{RN104,
   author = {Traore, B. and Pedesseau, L. and Assam, L. and Che, X. and Blancon, J. C. and Tsai, H. and Nie, W. and Stoumpos, C. C. and Kanatzidis, M. G. and Tretiak, S. and Mohite, A. D. and Even, J. and Kepenekian, M. and Katan, C.},
   title = {Composite Nature of Layered Hybrid Perovskites: Assessment on Quantum and Dielectric Confinements and Band Alignment},
