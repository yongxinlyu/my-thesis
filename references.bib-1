}

@article{RN419,
   author = {Gusev, V. V. and Adamson, D. and Deligkas, A. and Antypov, D. and Collins, C. M. and Krysta, P. and Potapov, I. and Darling, G. R. and Dyer, M. S. and Spirakis, P. and Rosseinsky, M. J.},
   title = {Optimality guarantees for crystal structure prediction},
